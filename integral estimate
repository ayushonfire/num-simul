
#include <stdio.h>
#include<math.h>

int main()
{
    double a=1.8,b=3.4,n=8,i,ar[10],s=0.0;
    int j=0;
    printf("\n enter the elements :\n");
    for(i=1.8;i<=3.6;i+=0.2)
    {
        scanf("%lf",&ar[j++]);
        
    }
    for(j=1;j<=7;j++)
    {
        ar[j]*=2;
    }
    for(j=0;j<9;j++)
    {
        s=s+ar[j];
        
    }
    double h=(b-a)/n;
    printf("\n the estimate of the integral is : \n");
    printf("%lf",(h/2)*s);
    
    
}
